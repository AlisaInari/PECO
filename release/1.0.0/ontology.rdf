        <vann:preferredNamespaceUri>https://w3id.org/peco#</vann:preferredNamespaceUri>
        <owl:priorVersion rdf:resource="https://w3id.org/peco/0.0.1"/>
        <owl:versionInfo xml:lang="en">1.0.0</owl:versionInfo>
    </owl:Ontology>
    


